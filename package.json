{
  "name": "leaky-bucket-rate-limiter",
  "version": "1.0.0",
  "description": "A TypeScript implementation of a leaky bucket rate limiter",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist",
    "prepare": "husky"
  },
  "keywords": [
    "rate-limiter",
    "leaky-bucket",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  }
}
